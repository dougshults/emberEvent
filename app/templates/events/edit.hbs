<h2>Edit Event </h2>
<form>
<div class="form-group">
<label>Title</label>
{{input type="text" class="form-control" value=model.title placeholder="Add Event..."}}
</div>
<div class="form-group">
<label>Event Date</label>
{{input type="date" class="form-control" value=model.date}}
</div>
<div class="form-group">
<label>Description</label>
{{textarea class="form-control" value=model.description placeholder="Describe Event..."}}
</div>
<button {{action 'editEvent' model.id}}class="btn btn-primary">Submit</button>
</form>
